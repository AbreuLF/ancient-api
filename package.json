{
  "name": "agames-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "lint": "yarn eslint ./src/ --ext .js,.jsx,.ts,.tsx",
    "tsc": "tsc",
    "start": "tsc && ts-node ./src/server.ts",
    "dev": "npx nodemon -r dotenv/config public/server.js",
    "test": "./node_modules/.bin/mocha",
    "db:migrate": "tsc && rm -rf public/database/seeders/*.js.map && rm -rf public/database/migrations/*.js.map && sequelize db:migrate",
    "db:migrate:create": "sequelize migration:generate",
    "db:seed": "tsc && rm -rf public/database/seeders/*.js.map && rm -rf public/database/migrations/*.js.map && sequelize db:seed:all",
    "db:seed:create": "sequelize seed:generate"
  },
  "author": "Luiz Abreu",
  "license": "ISC",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/node": "^14.0.26",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.3.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "0.0.4",
    "jsdoc": "^3.6.4",
    "mocha": "^8.0.1",
    "source-map-loader": "^1.0.1",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/faker": "^4.1.12",
    "@types/mocha": "^7.0.2",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.0",
    "apollo-server-express": "^2.16.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "faker": "^4.1.0",
    "graphql": "^15.3.0",
    "http": "0.0.1-security",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "5.22.3",
    "sequelize-cli-typescript": "git+ssh://git@github.com:wildcookie007/sequelize-cli-typescript.git#master",
    "sequelize-typescript": "^1.1.0"
  }
